{
    "name": "agence-adeliom/easy-common-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for EasyAdmin that provide basic entity traits and a enum helper",
    "keywords": ["symfony", "easyadmin", "enum", "helper"],
    "homepage": "https://github.com/agence-adeliom/easy-common-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud RITTI",
            "homepage": "https://github.com/arnaud-ritti"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "ext-json": "*",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "easycorp/easyadmin-bundle": "^3.4 || ^v4.0"
    },
    "autoload": {
        "psr-4": {
            "Adeliom\\EasyCommonBundle\\": "src/"
        }
    },
    "conflict": {
        "agence-adeliom/easy-faq-bundle": "<6.0.1",
        "agence-adeliom/easy-seo-bundle": "<6.0.1",
        "agence-adeliom/easy-config-bundle": "<6.0.1",
        "agence-adeliom/easy-redirect-bundle": "<6.0.1",
        "agence-adeliom/easy-blog-bundle": "<6.0.1",
        "agence-adeliom/easy-media-bundle": "<6.0.1",
        "agence-adeliom/easy-fields-bundle": "<6.0.1",
        "agence-adeliom/easy-menu-bundle": "<6.0.1",
        "agence-adeliom/easy-page-bundle": "<6.0.1",
        "agence-adeliom/easy-block-bundle": "<6.0.1",
        "agence-adeliom/easy-editor-bundle": "<6.0.1",
        "agence-adeliom/easy-admin-user-bundle": "<6.0.1",
        "agence-adeliom/easy-gutenberg-bundle": "<6.0.1"
    }
}
